  <PropertyGroup>
    <PostBuildEvent>REM ===========================================
REM Remove trailing slash off ProjectDir as NUget doesn't like it
REM ===========================================

SET targetDir=$(ProjectDir)
SET endChar=%25targetDir:~-1%25
IF %25endChar%25 == \ SET targetDir=%25targetDir:~0,-1%25

echo targetDir = "%25targetDir%25"

REM ===========================================
REM Detect actual OutputDir (Differs in VS and Build script)
REM ===========================================

SET webProjectOutputDir=$(WebProjectOutputDir)
IF "%25webProjectOutputDir%25" == "" SET webProjectOutputDir=$(SolutionDir)Web Apps\Umbraco.Cms.Web.UI\

echo webProjectOutputDir = "%25webProjectOutputDir%25"

REM ===========================================
REM Decide which NUget template to use based on build config
REM ===========================================

SET config=$(ConfigurationName)
SET template=Debug
SET configChar=%25config:~0,1%25
IF %25configChar%25 == R SET template=Release
IF NOT "$(WebProjectOutputDir)" == "" SET template=Build

echo template = "%25template%25"

REM ===========================================
REM Create NUget package
REM ===========================================

"$(SolutionDir)..\Resources\Build\Nuget\Nuget.exe" pack "%25targetDir%25\Nuspecs\DevDataset.1.0.%25template%25.nuspec" -BasePath "%25targetDir%25" -OutputDirectory "%25webProjectOutputDir%25App_Data\Umbraco\LocalPackages"

</PostBuildEvent>
