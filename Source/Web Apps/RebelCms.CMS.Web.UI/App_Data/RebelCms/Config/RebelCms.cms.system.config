<?xml version="1.0"?>
<system>  

  <public-packages repositoryAddress="http://packages.RebelCms.com" />
    
  <folders
    backOfficeFolder="~/Areas/RebelCms"
    
    docTypeIconFolder="~/Areas/RebelCms/Content/Images/DocTypeIcons"
    docTypeThumbnailFolder="~/Areas/RebelCms/Content/Images/DocTypeThumbs"    
    appIconFolder="~/Areas/RebelCms/Content/Images/AppIcons"
    
    scriptsFolder="~/Scripts/RebelCms"
    stylesheetsFolder="~/Content/Styles"
    templateFolder="~/Views/RebelCms"
    localPackageRepositoryFolder="~/App_Data/RebelCms/LocalPackages"/>

  <urls removeDoubleDashes="true" stripNonAscii="true" urlEncode="false">
    <charReplacements>
      <add char=" " value="-" />
      <add char="&quot;" value="" />
      <add char="'" value="" />
      <add char="%" value="" />
      <add char="," value="" />
      <add char="." value="" />
      <add char=";" value="" />
      <add char="/" value="" />
      <add char=":" value="" />
      <add char="#" value="" />
      <add char="?" value="" />
      <add char="+" value="plus" />
      <add char="*" value="star" />
      <add char="&amp;" value="and" />
      <add char="æ" value="ae" />
      <add char="ø" value="oe" />
      <add char="å" value="aa" />
      <add char="ä" value="ae" />
      <add char="ö" value="oe" />
      <add char="ü" value="ue" />
      <add char="ß" value="ss" />
      <add char="Ä" value="ae" />
      <add char="Ö" value="oe" />
    </charReplacements>
  </urls>
  
  <paths
    backOfficePath="RebelCms" localizationPath="Localization" />

  <routeMatches>      
    <add route="/Error" type="Exclude" />
    <add route="/RebelCms/Error/Msg" type="Exclude" />
    <add route="/Scripts" type="Exclude" /> 
    
    <add route="*" type="Include" />
    <add route="/an-excluded-path*" type="Exclude" />
  </routeMatches>
  
</system>
